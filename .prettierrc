{
  "printWidth": 110,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 90,
        "trailingComma": "none",
        "proseWrap": "always"
      }
    },
    {
      "files": "*.scss",
      "options": {
        "tabWidth": 4,
        "printWidth": 90
      }
    }
  ]
}
