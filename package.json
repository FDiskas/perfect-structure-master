{
  "name": "parcel2",
  "version": "1.0.0",
  "description": "",
  "main": "src/client/main.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel src/client/index.html --no-autoinstall --open",
    "build": "cross-env NODE_ENV=production rimraf dist && parcel build src/client/index.html --no-source-maps --no-cache --experimental-scope-hoisting",
    "build:serve": "serve dist -s -n",
    "pretest": "npm run lint:css && npm run lint && tsc --noEmit",
    "test": "npm run test:jest",
    "posttest": "npm run format",
    "format": "prettier --write \"**/*.{js,css,md}\"",
    "lint": "eslint src --fix --ext .ts,.tsx",
    "lint:css": "stylelint 'src/**/*.scss'",
    "test:jest": "jest --noStackTrace --notify",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "styleguide": "styleguidist server --config .config/styleguide.config.js --open",
    "styleguide:build": "styleguidist build --config .config/styleguide.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@parcel/logger": "^1.11.0",
    "@types/jest": "^24.0.11",
    "@types/lodash-es": "^4.17.3",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^13.1.0",
    "node-sass": "^7.0.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-typed-css-modules": "^1.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.17.0",
    "react-docgen-typescript": "^1.12.4",
    "react-hot-loader": "^4.8.4",
    "react-styleguidist": "^9.0.8",
    "rimraf": "^2.6.3",
    "sass": "^1.19.0",
    "sass-loader": "^7.1.0",
    "serve": "^11.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-prettier": "^1.0.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.4",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.30.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "lodash-es": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "remove": "^0.1.5"
  }
}
